{
  "name": "jexy/dummy",
  "type": "library",
  "license": "MIT",
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist",
    "sort-packages": true,
    "prepend-autoloader": false,
    "classmap-authoritative": true
  },
  "description": "Jexy Dummy plugin for WordPress",
  "autoload": {
    "psr-4": {
      "Jexy\\Dummy\\": [
        "plugin/includes/",
        "plugin/admin/",
        "plugin/public/",
        "blocks/src/"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dubyaio/foundation.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dubyaio/plugin-updater-github.git"
    }
  ],
  "require": {
    "dubya/foundation": "dev-next",
    "dubya/plugin-updater-github": "dev-beta"
  }
}